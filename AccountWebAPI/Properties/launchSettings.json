{
  "profiles": {
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7280",
      "launchUrl": "https://localhost:7280/swagger/index.html"
    },
    "Docker": {
      "commandName": "Docker",
      "applicationUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "launchBrowser": true,
      "launchUrl": "https://localhost:{ServicePort}/swagger/index.html"
    }
  }
}